    <dependencies>
      <group targetFramework="net45">
          <dependency id="Newtonsoft.Json" version="[7,8)" />
          <dependency id="Common.Logging" version="3.2.0" />
      </group>
    </dependencies>
  </metadata>

  <files>
      <file src="RethinkDb.Driver.dll" target="lib\net45" />
