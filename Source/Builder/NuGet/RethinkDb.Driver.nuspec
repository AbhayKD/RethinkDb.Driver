<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>RethinkDb.Driver</id>
    <version>0.0.0.0</version>
    <authors>Brian Chavez</authors>
    <owners>Brian Chavez</owners>
    <licenseUrl>https://raw.githubusercontent.com/bchavez/RethinkDb.Driver/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/bchavez/RethinkDb.Driver</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/bchavez/RethinkDb.Driver/master/Docs/logo.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    
    <summary>A RethinkDB database driver written in C# striving for 100% ReQL API compatibility and completeness. Architecturally, this driver is a one-to-one port of the official Java driver.</summary>
    
    <description>A RethinkDB database driver written in C# striving for 100% ReQL API compatibility and completeness. Architecturally, this driver is a one-to-one port of the official Java driver.</description>
    
    <releaseNotes>
        @history@
    </releaseNotes>
    <copyright>Copyright 2015</copyright>
    <tags>rethinkdb database driver reql</tags>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="Newtonsoft.Json" version="[7,8)" />
        <dependency id="Common.Logging" version="3.3.0" />
      </group>
      <group targetFramework=".NETPlatform5.0">
        <dependency id="Newtonsoft.Json" version="7.0.1" />
        <dependency id="System.Text.Encoding" version="4.0.11-beta-23405" />
        <dependency id="System.Dynamic.Runtime" version="4.0.10-beta-23109" />
        <dependency id="System.IO" version="4.0.11-beta-23225" />
        <dependency id="System.Reflection.Extensions" version="4.0.1-beta-23225" />
        <dependency id="System.ComponentModel" version="4.0.1-beta-23405" />
        <dependency id="Microsoft.Extensions.Logging" version="1.0.0-rc1-15673" />
        <dependency id="System.Net.Sockets" version="4.0.10-beta-23109" />
      </group>
    </dependencies>
  </metadata>

  <files>
    <file src="Release\net45\RethinkDb.Driver.dll" target="lib\net45" />
    <file src="Release\net45\RethinkDb.Driver.pdb" target="lib\net45" />
    <file src="Release\net45\RethinkDb.Driver.XML" target="lib\net45" />

    <file src="Release\dotnet\RethinkDb.Driver.dll" target="lib\dotnet" />
    <file src="Release\dotnet\RethinkDb.Driver.pdb" target="lib\dotnet" />
    <file src="Release\dotnet\RethinkDb.Driver.XML" target="lib\dotnet" />

    <file src="..\..\Source\RethinkDb.Driver\**\*.cs"
            exclude="..\..\Source\RethinkDb.Driver\obj\**" target="src" />
  </files>

</package>
