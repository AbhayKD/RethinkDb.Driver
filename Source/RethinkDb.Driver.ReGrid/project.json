  "authors": [
    "Brian Chavez"
  ],
  "tags": [
    ""
  ],
  "projectUrl": "https://github.com/bchavez/RethinkDb.Driver",
  "licenseUrl": "https://raw.githubusercontent.com/bchavez/RethinkDb.Driver/master/LICENSE",
  "configurations": {
    "Debug": {
      "compilationOptions": {}
    },
    "Release": {
      "compilationOptions": {
        "optimize": true
      }
    }
  },
  "dependencies": {
    "Newtonsoft.Json": "8.0.2",
    "RethinkDb.Driver": ""
  },
  "frameworks": {
    "net45": {
      "dependencies": {
        "Common.Logging": "3.3.0"
      },
      "frameworkAssemblies": {}
    },
    "dnx451": {
      "dependencies": {
        "Microsoft.Extensions.Logging.Abstractions": "1.0.0-rc1-final"
      },
      "compilationOptions": {
