@using System.Collections.Generic
@using System.Linq

@functions {
    public string EnumName { get; set; }
    public Dictionary<string, object> Enums { get; set; }
}

//AUTOGENERATED, DO NOTMODIFY.
//Do not edit this file directly.

// ReSharper disable CheckNamespace

namespace RethinkDb.Driver.Proto {
    public enum @EnumName {
    @foreach( var kvp in this.Enums )
    {
        @:@kvp.Key = @kvp.Value,
    }
    }

    @if( EnumName == "ResponseNote" )
    {
        @:public static class @("ExtensionsFor" + EnumName) {
            @:public static bool IsFeed(this ResponseNote note){
                @: switch( note ){
                foreach( var k in Enums.Keys.Where(k => k.EndsWith("FEED") ))
                {
                    @: case ResponseNote.@k:
                        @:return true;
                }
                    @:default:
                        @:return false;
                @:}
            @:}
        @:}
    }
    @if( EnumName == "ResponseType" )
    {
        @:public static class @("ExtensionsFor" + EnumName) {
            @:public static bool IsError(this ResponseType t){
                @: switch( t ){
                foreach( var k in Enums.Keys.Where(k => k.EndsWith("ERROR")) )
                {
                    @: case ResponseType.@k:
                        @:return true;
                }
                    @:default:
                        @:return false;
                @:}
            @:}
        @:}
    }

}
